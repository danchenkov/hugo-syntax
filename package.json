{
  "name": "hugo-syntax",
  "displayName": "Hugo Template Syntax Support",
  "description": "Syntax highlighting for Hugo HTML templates and content files",
  "icon": "icon.png",
  "version": "0.0.114",
  "author": "Alexei Danchenkov",
  "publisher": "adanchenkov",
  "license": "MIT",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "hugo",
        "aliases": [
          "Hugo Template",
          "Hugo"
        ],
        "extensions": [
          ".html",
          ".hugo"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hugo",
        "scopeName": "text.html.hugo",
        "path": "./syntaxes/hugo.tmLanguage.json"
      }
    ]
  },
  "scripts": {},
  "devDependencies": {
    "@types/vscode": "^1.97.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danchenkov/hugo-syntax"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}